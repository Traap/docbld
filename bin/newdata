#/!data/bash
#
# This script refreshes your document data directory. 

# Copy files used by tlc-article to layout document header and document footer
# to the data directory.
cp -v ${DOCBLDPATH}/data/additional-layout.tex data/additional-layout.tex
cp -v ${DOCBLDPATH}/data/header-footer.tex     data/header-footer.tex
cp -v ${DOCBLDPATH}/data/logo.png              data/logo.png
cp -v ${DOCBLDPATH}/data/version.tex           data/version.tex
